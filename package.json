{
  "name": "@aiou/bin-template",
  "version": "2.0.6",
  "description": "templates for command project",
  "license": "MIT",
  "homepage": "https://github.com/JiangWeixian/templates#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JiangWeixian/templates.git",
    "directory": "packages/bin-template"
  },
  "bugs": {
    "url": "https://github.com/JiangWeixian/templates/issues",
    "email": "jiangweixian1994@gmail.com"
  },
  "author": {
    "name": "JW",
    "email": "jiangweixian1994@gmail.com",
    "url": "https://twitter.com/jiangweixian"
  },
  "main": ".lib/bin-template.cjs.js",
  "module": ".lib/bin-template.esm.js",
  "bin": {
    "bin-template": "lib/bin-template.js"
  },
  "scripts": {
    "prebuild": "rimraf lib",
    "build": "tsc",
    "pretest": "pnpm run build",
    "ci:publish": "pnpm run build && pnpx changeset publish",
    "ci:version": "pnpx changeset version",
    "ci:snapshot": "pnpx changeset version --snapshot beta",
    "ci:prerelease": "pnpm run build && pnpx changeset publish --tag beta",
    "lint:fix": "eslint . --fix",
    "prepare": "husky install"
  },
  "lint-staged": {
    "**/**/*.{js,ts,tsx,vue,json}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "chalk": "4.1.1",
    "commander": "8.0.0",
    "inquirer": "8.1.2",
    "ora": "5.4.1"
  },
  "devDependencies": {
    "@aiou/eslint-config": "0.2.1",
    "@changesets/cli": "^2.16.0",
    "@types/download": "8.0.1",
    "@types/fs-extra": "9.0.12",
    "@types/inquirer": "7.3.3",
    "@types/node": "16.3.3",
    "@types/rimraf": "3.0.1",
    "cz-emoji": "^1.3.1",
    "eslint": "^7.30.0",
    "husky": "^7.0.0",
    "lint-staged": "^11.0.1",
    "np": "7.5.0",
    "prettier": "2.3.2",
    "pretty-quick": "3.1.1",
    "rimraf": "3.0.2",
    "typescript": "4.3.5"
  },
  "config": {
    "commitizen": {
      "path": "cz-emoji"
    }
  }
}
